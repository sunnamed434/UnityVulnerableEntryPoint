<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <Version>0.1.0</Version>
        <Copyright>sunnamed434</Copyright>
        <RepositoryUrl>https://github.com/sunnamed434/BitMono</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AsmResolver" Version="5.1.0" PrivateAssets="All" />
        <PackageReference Include="AsmResolver.DotNet" Version="5.1.0" PrivateAssets="All" />
        <PackageReference Include="CommandLineParser" Version="2.9.1" PrivateAssets="All" />
        <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>

</Project>
